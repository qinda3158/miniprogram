/* pages/search/result.wxss */
.expert-item {
  width: 100%;
  height: 200rpx;
  padding-left: 30rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  /* background-color: pink; */
  margin-bottom: 10rpx;
}

.expert-info {
  padding-left: 30rpx;
}

.expert-img {
  width: 120rpx;
  height: 120rpx;
  background-color: #ffffff;
  border: solid 2rpx #f4f5f9;
  border-radius: 50%;
  overflow: hidden;
  /* background-color: pink; */
  flex-shrink: 0;
}

.expert-img image {
  width: 100%;
  height: 100%;
}

.expert-name {
  height: 32rpx;
  line-height: 32rpx;
  font-weight: 700;
  color: #222;
}

.expert-desc {
  height: 24rpx;
  line-height: 24rpx;
  white-space: normal;
  color: #999;
  margin-top: 24rpx;
  margin-bottom: 14rpx;
  font-size: 24rpx;
  line-height: 24rpx;
}

.expert-desc .h {
  font-weight: 700;
  color: #333;
}

.expert-desc .l {
  color: #eb4f53;
}

.expert-position {
  height: 22rpx;
  line-height: 22rpx;
  font-size: 22rpx;
  color: #999;
}

/* 案例条目 */
.case-item {
  width: 692rpx;
  height: 592rpx;
  box-sizing: border-box;
  background-color: pink;
  margin: 0 auto;
}

.user {
  height: 58rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 30rpx;
}

.user-img {
  width: 58rpx;
  height: 58rpx;
  border-radius: 50%;
  overflow: hidden;
}

.user-img image {
  width: 100%;
  height: 100%;
}

.case-img {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 30rpx;
  border-radius: 12rpx;
  overflow: hidden;
}

.img-before,
.img-after {
  width: 342rpx;
  height: 342rpx;
  /* background-color: cyan; */
  position: relative;
}

.img-before image,
.img-after image {
  width: 100%;
  height: 100%;
}

.case-desc {
  height: 74rpx;
  /* background-color: cyan; */
  font-size: 28rpx;
  color: #333;
  line-height: 40rpx;
  margin-bottom: 30rpx;
}

.case-item-name {
  height: 28rpx;
  font-size: 28rpx;
  line-height: 28rpx;
  color: #999;
}

.before,
.after {
  height: 40rpx;
  display: inline-block;
  line-height: 40rpx;
  padding: 0 16rpx;
  border-radius: 20rpx;
  font-size: 20rpx;
  color: #fff;
  background-color: rgba(0, 0, 0, .2);
  position: absolute;
  bottom: 16rpx;
  left: 20rpx;
}

