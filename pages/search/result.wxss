.anser-outer {
  width: 690rpx;
  height: 370rpx;
  background-color: pink;
  margin: 0 auto;
}

.anser-outer .title {
  height: 32rpx;
  font-size: 32rpx;
  line-height: 32rpx;
  color: #222;
  font-weight: 700;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.doctor {
  height: 40rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 24rpx;
  color: #999;
  margin-top: 32rpx;
  margin-bottom: 56rpx;
}

.doctor .user-img {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 16rpx;
}

.doctor .user-img image {
  width: 100%;
  height: 100%;
}

.point {
  width: 2rpx;
  height: 2rpx;
  border-radius: 1rpx;
  background-color: #999;
  margin: 0 20rpx;
}

.doctor-name {
  font-size: 24rpx;
  color: #333;
}

.answer {
  width: 650rpx;
  height: 120rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
  /* background-color: cyan; */
  margin: 0 auto;
  font-size: 26rpx;
  color: #333;
  line-height: 42rpx;
  margin-bottom: 64rpx;
}

.bottom-num {
  height: 24rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 22rpx;
  color: #666;
}

.bottom-num .iconfont {
  font-size: 36rpx;
  vertical-align: middle;
}

