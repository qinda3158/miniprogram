/* pages/search/result.wxss */
page{
  background-color: #fbfcfb;
}
.search-outer {
  width: 690rpx;
  height: 68rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 auto;
  margin-top: 20rpx;
}

.search {
  width: 610rpx;
  height: 68rpx;
  font-size: 26rpx;
  color: #999;
  background-color: #f2f5f7;
  border-radius: 8rpx;
  position: relative;
  line-height: 60rpx;
  padding-left: 70rpx;
  padding-right: 20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  /* background-color: pink; */
}

.search input {
  width: 100%;
  height: 100%;
}

.input-icon {
  width: 24rpx;
  height: 27rpx;
  position: absolute;
  top: 50%;
  left: 32rpx;
  transform: translateY(-50%);
}

.cancel-input {
  width: 34rpx;
  height: 34rpx;
  background-color: #c3c4c7;
  color: #fff;
  font-size: 30rpx;
  line-height: 30rpx;
  text-align: center;
  border-radius: 50%;
  flex-shrink: 0;
}

.cancel-btn {
  width: 60rpx;
  height: 100%;
  font-size: 28rpx;
  color: #666;
  letter-spacing: 1rpx;
  text-align: right;
  line-height: 68rpx;
}

/* tab栏 */
.tab-list {
  height: 126rpx;
  line-height: 126rpx;
  display: flex;
  justify-content: flex-start;
  background-color: #fff;
  border-bottom: 1rpx solid #f4f5f9;
}

.tab-item {
  height: 126rpx;
  padding: 0 34rpx;
  font-size: 28rpx;
  color: #999;
}

.tab-list .tab-item:nth-child(1) {
  padding-left: 30rpx;
}

.tab-item.active {
  color: #222;
  font-weight: 700;
  font-size: 36rpx;
}

/* 商品列表 */

/* 商品区域 */
.goods-list {
  width: 702rpx;
  padding-bottom: 16rpx;
  margin: 0 auto;
}

.goods-item {
  width: 702rpx;
  padding-left: 24rpx;
  padding-top: 40rpx;
  padding-right: 24rpx;
  height: 240rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-start;
  background-color: #fff;
  border-top: 1rpx solid #f4f5f9;
}

.goods-img {
  width: 160rpx;
  height: 160rpx;
  flex-shrink: 0;
}

.goods-img image {
  width: 100%;
  height: 100%;
}

.goods-desc {
  width: calc(100% - 160rpx);
  height: 160rpx;
  padding-left: 40rpx;
  padding-right: 24rpx;
  box-sizing: border-box;
}

.goods-title {
  height: 32rpx;
  line-height: 32rpx;
  font-size: 30rpx;
  color: #222;
  font-weight: 700;
  margin-bottom: 22rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  transform: translateX(-16rpx);
}

.goods-coupon {
  height: 30rpx;
  display: flex;
  justify-content: flex-start;
  margin-bottom: 38rpx;
}

.goods-coupon .coupon {
  width: 202rpx;
  height: 30rpx;
  background-color: #ffffff;
  border: solid 1rpx #eb4f53;
  display: flex;
  justify-content: flex-start;
  color: #eb4f53;
  font-size: 20rpx;
  text-align: center;
}

.goods-coupon .coupon .left {
  width: 78rpx;
  height: 100%;
  background-color: #ffecec;
  flex-shrink: 0;
}

.goods-coupon .coupon .right {
  width: 100%;
  height: 100%;
}

.goods-price {
  height: 26rpx;
  line-height: 26rpx;
  font-size: 32rpx;
  color: #eb4f53;
}

.goods-price text {
  font-size: 24rpx;
  color: #b7b8bd;
}

.goods-price .rmb {
  font-size: 20rpx;
  color: #eb4f53;
  line-height: 24rpx;
}

/* 专家-综合 */
.expert-outer {
  width: 100%;
  height: 310rpx;
  padding-bottom: 46rpx;
  display: flex;
  justify-content: flex-start;
  overflow: scroll;
  background-color: #fff;
}

.expert-i {
  width: 320rpx;
  height: 310rpx;
  /* background-color: pink; */
  overflow: hidden;
  flex-shrink: 0;
  white-space: nowrap;
  margin-right: 14rpx;
}

.info-outer {
  padding-top: 88rpx;
  height: 250rpx;
  box-sizing: border-box;
  background-origin: content-box;
  background-color: #f9faff;
  position: relative;
  margin-top: 62rpx;
  text-align: center;
  color: #777;
}
.expert-n{
  height: 30rpx;
  line-height: 30rpx;
  font-size: 28rpx;
  color: #222;
  font-weight: 700;
}
.tx {
  width: 124rpx;
  height: 124rpx;
  border-radius: 50%;
  background-color: cyan;
  position: absolute;
  top: -62rpx;
  left: 50%;
  transform: translateX(-50%);
}
.tx image{
  width: 100%;
  height: 100%;
}
.expert-d{
  margin-top: 24rpx;
  margin-bottom: 20rpx;
}
.expert-d,.expert-num{
  height: 22rpx;
  font-size: 22rpx;
  line-height: 22rpx;
}
.expert-outer .expert-i:first-child {
  padding-left: 30rpx;
}

.expert-outer .expert-i:last-child {
  padding-right: 30rpx;
}
::-webkit-scrollbar {
  height: 0;
  width: 0;
  color: transparent;
}
/* 专家 */
.expert-item {
  width: 100%;
  height: 200rpx;
  padding-left: 30rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  /* background-color: pink; */
  margin-bottom: 10rpx;
}

.expert-info {
  padding-left: 30rpx;
}

.expert-img {
  width: 120rpx;
  height: 120rpx;
  background-color: #ffffff;
  border: solid 2rpx #f4f5f9;
  border-radius: 50%;
  overflow: hidden;
  /* background-color: pink; */
  flex-shrink: 0;
}

.expert-img image {
  width: 100%;
  height: 100%;
}

.expert-name {
  height: 32rpx;
  line-height: 32rpx;
  font-weight: 700;
  color: #222;
}

.expert-desc {
  height: 24rpx;
  line-height: 24rpx;
  white-space: normal;
  color: #999;
  margin-top: 24rpx;
  margin-bottom: 14rpx;
  font-size: 24rpx;
  line-height: 24rpx;
}

.expert-desc .h {
  font-weight: 700;
  color: #333;
}

.expert-desc .l {
  color: #eb4f53;
}

.expert-position {
  height: 22rpx;
  line-height: 22rpx;
  font-size: 22rpx;
  color: #999;
}

/* 案例条目 */
.case-item {
  width: 692rpx;
  height: 662rpx;
  padding-top: 40rpx;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-bottom: 1rpx solid #f4f5f9;
  margin: 0 auto;
}

.user {
  height: 58rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 30rpx;
}

.user-img {
  width: 58rpx;
  height: 58rpx;
  border-radius: 50%;
  overflow: hidden;
}

.user-img image {
  width: 100%;
  height: 100%;
}

.case-img {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 30rpx;
  border-radius: 12rpx;
  overflow: hidden;
}

.img-before,
.img-after {
  width: 342rpx;
  height: 342rpx;
  /* background-color: cyan; */
  position: relative;
}

.img-before image,
.img-after image {
  width: 100%;
  height: 100%;
}

.case-desc {
  height: 74rpx;
  /* background-color: cyan; */
  font-size: 28rpx;
  color: #333;
  line-height: 40rpx;
  margin-bottom: 30rpx;
}

.case-item-name {
  height: 28rpx;
  font-size: 28rpx;
  line-height: 28rpx;
  color: #999;
}

.before,
.after {
  height: 40rpx;
  display: inline-block;
  line-height: 40rpx;
  padding: 0 16rpx;
  border-radius: 20rpx;
  font-size: 20rpx;
  color: #fff;
  background-color: rgba(0, 0, 0, .2);
  position: absolute;
  bottom: 16rpx;
  left: 20rpx;
}

/* 问答 */
